{
  "manifest_version": 3,
  "name": "Hello Extensions of the world",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "seeking.html",
    "default_icon": "seeking_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.bilibili.com/video/*"],
      "js": ["scripts/seek.js"]
    }
  ]
}